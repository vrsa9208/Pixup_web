<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>StoreServlet</servlet-name>
        <servlet-class>mx.com.pixup.portal.view.servlets.StoreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CatalogosServlet</servlet-name>
        <servlet-class>mx.com.pixup.portal.view.servlets.CatalogosServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>StoreServlet</servlet-name>
        <url-pattern>/store</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CatalogosServlet</servlet-name>
        <url-pattern>/catalogos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
